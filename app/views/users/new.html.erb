<h1>新用户</h1>
<h2>注册</h2>
<%= form_for :user, url: users_path do |f| %>
    <p>
      <%= f.label :username %><br>
      <%= f.text_field :username %>
    </p>
    <p>
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </p>
    <p>
      <%= f.label :nickname %><br>
      <%= f.text_field :nickname %>
    </p>

    <p>
      <%= f.label :contact %><br>
      <%= f.text_field :contact %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>